<h1>Podcast results</h1>


<ul>
  <% closest.forEach(result => { %>
    <li><h2><%= result.podcast.podcast %></h2></li>
    <li><h3><%= result.podcast.title %></h3></li>
    <li>
      <audio controls>
        <source src="<%= result.podcast.link %>" type="audio/mpeg">
      </audio>
    </li>

  <% }); %>
</ul>
