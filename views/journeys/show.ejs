<h1>Podcast results</h1>


<div class="carousel podResults">
  <% results.forEach(result => { %>
    <div class="carousel-item podcast">
    <div class="overlay">
    <p><%= result.title %></p>
    <audio controls>
      <source src="<%= result.link %>" type="audio/mpeg">
    </audio>
    </div>
    <img src="<%= result.image %>">

    </div>

  <% }); %>
</div>



<p>Your journey from <%= originText %> to <%= destinationText %> will take <%= journeyTime %> minutes, enjoy your podcasts</p>


